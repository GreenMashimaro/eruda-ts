{
  "name": "eruda-ts",
  "version": "1.0.0",
  "description": "eruda typescript version",
  "main": "index.js",
  "scripts": {
    "start": "node demo/start",
    "prepare": "pnpm run setup",
    "setup": "pnpm run build",
    "build": "run-p build:ts css:copy svg:copy",
    "watch": "run-p build watch:ts css:copy:watch svg:copy:watch",
    "build:ts": "tsc -b ./tsconfig.json",
    "watch:ts": "tsc -b -w ./tsconfig.json --preserveWatchOutput",
    "css:copy": "cpx 'src/**/*.{scss,css}' out/src",
    "css:copy:watch": "pnpm run css:copy --watch",
    "svg:copy": "cpx 'src/**/*.svg' out/src",
    "svg:copy:watch": "pnpm run svg:copy --watch",
    "test": "jest",
    "lint": "eslint \"src/**\" --ext \".ts\"",
    "lint:fix": "pnpm run lint --fix"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jest": "^29.4.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^9.7.4",
    "axios": "^1.3.4",
    "cpx": "^1.5.0",
    "css-loader": "^3.4.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.14.0",
    "eslint-plugin-vue": "^9.3.0",
    "express": "^4.18.1",
    "express-ws": "^5.0.2",
    "jest": "^29.5.0",
    "jest-scss-transform": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-clean": "^1.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-prefixer": "^2.1.2",
    "prettier": "^2.7.1",
    "sass": "^1.59.2",
    "sass-loader": "^13.0.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "webpack": "^5.61.0"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "licia": "^1.38.0",
    "luna-data-grid": "^0.4.3",
    "luna-dom-highlighter": "^1.0.2",
    "luna-dom-viewer": "^1.2.3",
    "luna-modal": "^1.0.0",
    "luna-notification": "^0.1.4",
    "luna-setting": "^0.7.0",
    "luna-tab": "^0.1.3",
    "luna-tabs": "^0.1.0",
    "node-pty": "^0.10.1",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.1.2",
    "typescript": "^5.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
